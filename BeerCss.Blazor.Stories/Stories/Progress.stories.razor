@attribute [Stories("Components/Progress")]

<Stories TComponent="Progress">
    <Story Name="Default">
        <Template>
            <Progress @attributes="context.Args" />
        </Template>
    </Story>

    <Story Name="Circle">
        <Template>
            <Progress @attributes="context.Args" Circle />
        </Template>
    </Story>

    <Story Name="30%">
        <Template>
            <Progress @attributes="context.Args" ProgressPercentage="30" />
        </Template>
    </Story>
</Stories>